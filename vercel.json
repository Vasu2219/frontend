{
  "version": 2,
  "name": "rapido-corporate-frontend-2025",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "GENERATE_SOURCEMAP": "false",
    "REACT_APP_API_URL": "https://rapido-backend-api.onrender.com/api",
    "REACT_APP_NAME": "Rapido Corporate",
    "REACT_APP_VERSION": "1.0.0"
  },
  "functions": {
    "app/build/**": {
      "maxDuration": 30
    }
  }
}
