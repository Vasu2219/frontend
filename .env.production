# Production Environment Configuration
REACT_APP_API_URL=https://rapido-backend-api.onrender.com/api
REACT_APP_API_TIMEOUT=15000

# App Configuration
REACT_APP_NAME=Rapido Corporate
REACT_APP_VERSION=1.0.0

# Environment
NODE_ENV=production

# React Query Configuration (in milliseconds)
REACT_APP_QUERY_STALE_TIME=60000
REACT_APP_QUERY_REFETCH_INTERVAL=60000
REACT_APP_QUERY_FAST_REFETCH_INTERVAL=30000
REACT_APP_QUERY_SLOW_REFETCH_INTERVAL=300000

# Socket.io configuration
REACT_APP_SOCKET_URL=https://rapido-backend-api.onrender.com

# Notification settings
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_NOTIFICATION_TIMEOUT=4000

# Build optimization
GENERATE_SOURCEMAP=false
CI=false
